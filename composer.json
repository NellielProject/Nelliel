{
	"name" : "nellielproject/nelliel",
	"description" : "Nelliel Imageboard",
	"require" : {
		"php" : ">=7.2",
		"ext-gd" : "*",
		"ext-dom" : "*",
		"ext-iconv" : "*",
		"ext-libxml" : "*",
		"ext-pdo" : "*",
		"ext-session" : "*",
		"s1lentium/iptools" : "~1.1.1",
		"cebe/markdown" : "~1.2.1",
		"psr/log" : "~1.1.4",
		"mustache/mustache" : "~2.14",
		"nikic/fast-route" : "~1.3",
		"monolog/monolog" : "~2.3",
		"lamansky/fraction" : "~1.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~8.5.21"
	},
	"type" : "project",
	"license" : "BSD-3-Clause",
	"keywords" : [
		"imageboard",
		"nelliel",
		"futallaby"
	],
	"authors" : [{
			"name" : "OtakuMegane",
			"homepage" : "https://github.com/OtakuMegane"
		}
	],
	"support" : {
		"source" : "https://github.com/NellielProject/Nelliel",
		"issues" : "https://github.com/NellielProject/Nelliel/issues"
	},
	"autoload" : {
		"psr-4" : {
			"Nelliel\\" : "core/include",
			"SmallPHPGettext\\" : "core/libraries/SmallPHPGettext",
			"phpDOMExtend\\" : "core/libraries/phpDOMExtend",
			"" : "core/libraries/portable-utf8"
		}
	}
}